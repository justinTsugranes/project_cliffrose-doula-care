    "6ddadc23-415d-40e0-9d75-46f266ddffdd": {
      "type": "image-with-text",
      "blocks": {
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-0": {
          "type": "heading",
          "settings": {
            "heading": "As full spectrum doulas…",
            "heading_size": "h1"
          }
        },
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-1": {
          "type": "text",
          "settings": {
            "text": "<p>We work with all reproductive journeys:<br\/>conception, adoption, surrogacy, pregnancy, labor, and birth. We are there in the postpartum and can help you through a termination process.<\/p><p><strong>We support people through every outcome.<\/strong><\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-0",
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-1"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/pregnant-woman-s-belly-closeup-hands-hugging-bell-2022-10-11-19-51-50-utc.jpg",
        "height": "adapt",
        "desktop_image_width": "medium",
        "layout": "text_first",
        "desktop_content_position": "top",
        "desktop_content_alignment": "left",
        "content_layout": "overlap",
