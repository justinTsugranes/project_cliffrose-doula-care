    "6ddadc23-415d-40e0-9d75-46f266ddffdd": {
      "type": "image-with-text",
      "blocks": {
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-0": {
          "type": "heading",
          "settings": {
            "heading": "As a full spectrum doula…",
            "heading_size": "h1"
          }
        },
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-1": {
          "type": "text",
          "settings": {
            "text": "<p>I work with all reproductive journeys:<br\/>conception, adoption, surrogacy, pregnancy, labor, and birth. I am there in the postpartum and can help you through a termination process.<\/p><p><strong>I support people through every outcome.<\/strong><\/p>",
            "text_style": "body"
          }
        }
      },
