    "6ddadc23-415d-40e0-9d75-46f266ddffdd": {
      "type": "image-with-text",
      "blocks": {
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-0": {
          "type": "heading",
          "settings": {
            "heading": "I am a full-spectrum doula",
            "heading_size": "h1"
          }
        },
        "template--18764259295534__6ddadc23-415d-40e0-9d75-46f266ddffdd-1681639870eeeda013-1": {
          "type": "text",
          "settings": {
            "text": "<p>I provide individual-centered care for all reproductive experiences, including conception, adoption, surrogacy, pregnancy, labor, birth, and termination.<\/p><p>My mission is to support people through every outcome with dignity, respect, and compassion.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
