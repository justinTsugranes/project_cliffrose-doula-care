    "0f55ffa0-e50a-4acf-ad18-997356e50af7": {
      "type": "image-with-text",
      "blocks": {
        "template--18771812843822__0f55ffa0-e50a-4acf-ad18-997356e50af7-1682184832658d0e18-0": {
