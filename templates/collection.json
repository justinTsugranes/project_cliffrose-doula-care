    "c09d92e5-2a87-408a-8087-801589bc779e": {
      "type": "image-banner",
      "blocks": {
        "template--18729374253358__c09d92e5-2a87-408a-8087-801589bc779e-1681573451f0a6c354-0": {
          "type": "heading",
          "settings": {
            "heading": "Birth Doula Services",
            "heading_size": "h1"
          }
        },
        "template--18729374253358__c09d92e5-2a87-408a-8087-801589bc779e-1681573451f0a6c354-1": {
          "type": "text",
          "settings": {
            "text": "As a full spectrum doula, I work with all reproductive journeys: conception, adoption, surrogacy, pregnancy, labor, and birth. I am there in the postpartum and can help you through a termination process. <strong>I support people through every outcome.<\/strong>",
            "text_style": "body"
          }
        },
        "template--18729374253358__c09d92e5-2a87-408a-8087-801589bc779e-1681573451f0a6c354-2": {
          "type": "buttons",
